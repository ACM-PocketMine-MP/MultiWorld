{
  "name": "czechpmdevs/multiworld",
  "description": "World management plugin for PocketMine servers",
  "minimum-stability": "dev",
  "license": "Apache-2.0",
  "require": {
    "php": "^8.0",
    "ext-yaml": "*",
    "ext-pthreads": "*",
    "ext-json": "*",
    "czechpmdevs/libpmform": "dev-feature/4.0.0",
    "pocketmine/pocketmine-mp": "dev-master"
  },
  "require-dev": {
    "phpstan/phpstan": "0.12.93",
    "friendsofphp/php-cs-fixer": "v2.19.0"
  },
  "repositories": [
    {
      "type": "vcs",
      "name": "czechpmdevs/libpmform",
      "url": "https://github.com/CzechPMDevs/libpmform"
    },
    {
      "type": "vcs",
      "name": "phpstan/phpstan",
      "url": "https://github.com/phpstan/phpstan"
    }
  ],
  "autoload": {
    "psr-4": {
      "": ["src"]
    }
  }
}